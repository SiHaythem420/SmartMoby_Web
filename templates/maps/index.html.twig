
{% block title %}Google Maps{% endblock %}

{% block body %}
    <h5 class="mb-0"><strong>Google Maps</strong></h5>
    <span class="text-secondary">Dashboard <i class="fa fa-angle-right"></i> Maps</span>

    <div class="row mt-3">
        <div class="col-12">
            <div class="card shadow-sm">
                <div class="card-body">
                    <iframe 
                        width="100%" 
                        height="600" 
                        frameborder="0" 
                        scrolling="no" 
                        marginheight="0" 
                        marginwidth="0" 
                        src="https://maps.google.com/maps?width=100%25&amp;height=600&amp;hl=en&amp;q=+(SmartMoby)&amp;t=&amp;z=14&amp;ie=UTF8&amp;iwloc=B&amp;output=embed"
                    >
                    </iframe>
                </div>
            </div>
        </div>
    </div>
{% endblock %} 